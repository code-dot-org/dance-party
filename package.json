    "karma-tap-pretty-reporter": "^4.1.0",
    "karma-webpack": "4.0.0-rc.2",
    "nyc": "^13.1.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.3",
    "raw-loader": "^0.5.1",
    "sinon": "^7.1.1",
    "tape": "^4.9.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
